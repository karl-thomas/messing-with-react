{
	"extends": ["plugin:flowtype/recommended", "airbnb", "prettier", "prettier/react"],
	"plugins": ["flowtype", "prettier"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"react/require-default-props": 0,
		"no-param-reassign": 0,
		"no-return-assign": 0,
		"react/forbid-prop-types": 0
	}
}
